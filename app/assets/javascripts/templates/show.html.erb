<section class="row" >
  <h1 class="text-center h2">You are now viewing playlist of {{user.full_name}}</h1>
  <ul class='list-unstyled'>
    <li	ng-repeat='song in songs'>
      <section class="well col-md-6 col-md-offset-3">
        <h1 class="h3 col-md-6 text-right" style="margin-top: 0"><a href='#'>{{song.url}}</a></h1>
        <div class="col-md-6">
        </div>
      </section>
    </li>
  </ul>
</section>
