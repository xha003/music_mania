controllers = angular.module('controllers')
controllers.controller("SongController", [ '$scope', '$routeParams', '$location', 'Song',
  ($scope,$routeParams,$location,Song)->
    Song.get({songId: $routeParams.songId},
      ((song) ->
        $scope.song = song
        MM.VideoHandler.viewSong($scope.song)
      ),
      (httpResponse)-> $scope.song = null
    )
])

