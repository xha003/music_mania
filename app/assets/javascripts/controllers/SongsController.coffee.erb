controllers = angular.module('controllers')
controllers.controller("SongsController", [ '$scope', '$routeParams', '$location', 'Song',
  ($scope,$routeParams,$location,Song)->
    Song.current.getNext({ index: $routeParams.index },
      ((song) ->
        $scope.song = song
        MM.VideoHandler.viewSong($scope.song)
      ),
      (httpResponse)-> $scope.song = null
    )
])

